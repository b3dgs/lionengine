<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<lionengine:formulas xmlns:lionengine="http://lionengine.b3dgs.com/xsd/10" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://lionengine.b3dgs.com/xsd/10 http://lionengine.b3dgs.com/xsd/10/game/feature/tile/map/collision/tileFormulas.xsd">
    <lionengine:formula name="ground">
        <lionengine:range maxX="16" maxY="0" minX="0" minY="0" output="Y"/>
        <lionengine:function a="0.0" b="0.0" type="LINEAR"/>
    </lionengine:formula>
    <lionengine:formula name="slope_left_0">
        <lionengine:range maxX="16" maxY="0" minX="0" minY="0" output="Y"/>
        <lionengine:function a="0.5" b="0.0" type="LINEAR"/>
    </lionengine:formula>
    <lionengine:formula name="slope_left_1">
        <lionengine:range maxX="16" maxY="0" minX="0" minY="0" output="Y"/>
        <lionengine:function a="0.5" b="8.0" type="LINEAR"/>
    </lionengine:formula>
    <lionengine:formula name="slope_right_0">
        <lionengine:range maxX="16" maxY="0" minX="0" minY="0" output="Y"/>
        <lionengine:function a="-0.5" b="8.0" type="LINEAR"/>
    </lionengine:formula>
    <lionengine:formula name="slope_right_1">
        <lionengine:range maxX="16" maxY="0" minX="0" minY="0" output="Y"/>
        <lionengine:function a="-0.5" b="16.0" type="LINEAR"/>
    </lionengine:formula>
    <lionengine:formula name="steep_left">
        <lionengine:range maxX="16" maxY="0" minX="0" minY="0" output="Y"/>
        <lionengine:function a="1.0" b="0.0" type="LINEAR"/>
    </lionengine:formula>
    <lionengine:formula name="steep_right">
        <lionengine:range maxX="16" maxY="0" minX="0" minY="0" output="Y"/>
        <lionengine:function a="-1.0" b="16.0" type="LINEAR"/>
    </lionengine:formula>
</lionengine:formulas>